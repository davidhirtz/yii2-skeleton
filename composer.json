{
	"name":"davidhirtz/yii2-skeleton",
	"type":"yii2-extension",
	"description":"Yii2 Skeleton",
	"homepage":"http://www.davidhirtz.com/",
	"minimum-stability":"dev",
	"prefer-stable":true,
	"require":{
		"php":">=5.4.0",
		"ext-exif":"*",
		"ext-json":"*",
		"ext-openssl":"*",
		"ext-simplexml":"*",
		"ext-xmlwriter":"*",
		"yiisoft/yii2":">=2.0.13",
		"yiisoft/yii2-bootstrap4":"*"
	},
	"autoload": {
		"psr-4": {
			"davidhirtz\\yii2\\skeleton\\": ""
		}
	},
	"scripts":{
		"post-install-cmd":[
			"yii\\composer\\Installer::postInstall"
		]
	},
	"extra":{
		"yii\\composer\\Installer::postInstall":{
			"setPermission":[
				{
					"runtime":"0777",
					"web/assets":"0777",
					"yii":"0755"
				}
			]
		}
	},
	"repositories":[
		{
			"type":"composer",
			"url":"https://asset-packagist.org"
		}
	],
	"extra":{
		"bootstrap":"davidhirtz\\yii2\\skeleton\\composer\\Bootstrap"
	}
}