{
	"name":"davidhirtz/yii2-skeleton",
	"type":"yii2-extension",
	"description":"Yii2 Skeleton",
	"homepage":"http://www.davidhirtz.com/",
	"minimum-stability":"dev",
	"prefer-stable":true,
	"require":{
		"php":">=7.0",
		"ext-exif":"*",
		"ext-json":"*",
		"ext-openssl":"*",
		"ext-simplexml":"*",
		"ext-xmlwriter":"*",
		"yiisoft/yii2":">=2.0.13",
		"yiisoft/yii2-authclient":"*",
		"yiisoft/yii2-bootstrap4":"*",
		"yiisoft/yii2-swiftmailer":"*",
		"yiisoft/yii2-imagine":">=2.1.0",
		"yiisoft/yii2-jui":"^2.0",
		"2amigos/yii2-file-upload-widget":"~1.0",
		"2amigos/yii2-ckeditor-widget":"~2.1",
		"rmrevin/yii2-fontawesome":"~3.2",
		"davidhirtz/yii2-datetime-behavior":"*",
		"davidhirtz/yii2-timeago":"dev-master",
		"davidhirtz/yii2-curl":"dev-master",
		"davidhirtz/yii2-lazysizes":"dev-master",
		"bower-asset/bootbox":">=4.4.0",
		"tinify/tinify":"^1.4",
		"guzzlehttp/guzzle":"~6.0",
		"guzzlehttp/oauth-subscriber":"0.3.*",
		"seld/cli-prompt": "^1.0",
		"npm-asset/@davidhirtz/jquery-router":"dev-master"
	},
	"autoload":{
		"psr-4":{
			"davidhirtz\\yii2\\skeleton\\":""
		}
	},
	"extra":{
		"bootstrap":"davidhirtz\\yii2\\skeleton\\composer\\Bootstrap"
	},
	"repositories":[
		{
			"type":"composer",
			"url":"https://asset-packagist.org"
		},
		{
			"type":"git",
			"url":"git@github.com:davidhirtz/jquery-router.git"
		}
	]
}