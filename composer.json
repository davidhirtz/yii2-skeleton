{
    "name": "davidhirtz/yii2-skeleton",
    "description": "Extended framework and admin panel based on Yii 2.0 framework",
    "license": "proprietary",
    "type": "yii2-extension",
    "homepage": "https://www.github.com/davidhirtz/yii2-skeleton",
    "authors": [
        {
            "name": "David Hirtz",
            "homepage": "https://www.davidhirtz.com/"
        }
    ],
    "require": {
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "davidhirtz/yii2-datetime-behavior": "^1.1",
        "davidhirtz/yii2-timeago": "^1.2",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/oauth-subscriber": "^0.5",
        "jfcherng/php-diff": "^6.9",
        "npm-asset/blueimp-file-upload": "^10.32",
        "npm-asset/bootbox": "^5",
        "npm-asset/jstimezonedetect": "^1.0",
        "robthree/twofactorauth": "^1.8",
        "seld/cli-prompt": "^1.0",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-authclient": "^2.2",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "yiisoft/yii2-imagine": "^2.1",
        "yiisoft/yii2-symfonymailer": "^2.0",
        "tinymce/tinymce": "^6.7.3"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/verify": "^3.0",
        "codeception/specify": "^2.0",
        "phpunit/phpunit": "^9",
        "codeception/module-yii2": "*",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*",
        "rector/rector": "^0.18.6",
        "symplify/easy-coding-standard": "^12.0",
        "yiisoft/yii2-debug": "^2.1",
        "codeception/module-filesystem": "^3.0"
    },
    "provide": {
        "bower-asset/inputmask": "~3.2.2|~3.3.5",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "~2.0.1"
    },
    "autoload": {
        "psr-4": {
            "davidhirtz\\yii2\\skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "davidhirtz\\yii2\\skeleton\\tests\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G",
        "test": "codecept run"
    }
}
